{
    "name": "chatette-vscode",
    "displayName": "Chatette Rasa",
    "description": "Syntax highlighting for chatette dataset of Rasa",
    "publisher": "cendekia",
    "repository": {
        "type": "git",
        "url": "https://github.com/cendekia/chatette-vscode.git"
    },
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.38.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "license": "MIT",
    "contributes": {
        "languages": [{
            "id": "chatette",
            "aliases": ["Chatette", "chatette"],
            "extensions": [".chatette"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "chatette",
            "scopeName": "source.chatette",
            "path": "./syntaxes/chatette.tmLanguage.json"
        }]
    }
}